        };

        num {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓

            display-name = "right";

            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
            //             ┏━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓   ┏━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓
            //             ┃     PSCRN     ┃   SCRL_LEFT   ┃   SCRL_DOWN   ┃    SCRL_UP    ┃   SCRL_RIGHT  ┃   ┃   SCRL_LEFT   ┃   SCRL_DOWN   ┃    SCRL_UP    ┃   SCRL_RIGHT  ┃     PSCRN     ┃
            //             ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫
            //             ┃      MB5      ┃      MB4      ┃      MB3      ┃      MB2      ┃      MB1      ┃   ┃      MB1      ┃      MB2      ┃      MB3      ┃      MB4      ┃      MB5      ┃
            // ┏━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━┓
            // ┃   PSCRN   ┃    CTRL+Z     ┃    CTRL+X     ┃    CTRL+C     ┃    CTRL+V     ┃  CTRL+SHFT+Z  ┃   ┃  CTRL+SHFT+Z  ┃    CTRL+V     ┃    CTRL+C     ┃    CTRL+X     ┃    CTRL+Z     ┃   PSCRN   ┃
            // ┗━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━┛
            //                                             ┃               ┃               ┃               ┃   ┃               ┃               ┃               ┃
            //                                             ┗━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┛   ┗━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┛

            bindings = <
       &trans  &kp C_PP  &kp C_PREV    &kp C_NEXT    &kp UNDER    &kp MINUS  &kp N7  &kp N8  &kp N9  &trans
       &trans  &trans    &kp C_VOL_DN  &kp C_VOL_UP  &kp PLUS     &kp EQUAL  &kp N4  &kp N5  &kp N6  &kp ENTER
&to 2  &trans  &kp AMPS  &kp FSLH      &kp STAR      &kp DOT      &kp N0     &kp N1  &kp N2  &kp N3  &trans     &to 5
                         &none         &to 0         &sk LCTRL    &sk RALT   &to 4   &none
            >;

            label = "num";
        };

        nav {
            // ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓

            display-name = "nav";

            // ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
            //             ┏━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓   ┏━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┓
            //             ┃       [       ┃       7       ┃       8       ┃       9       ┃       ]       ┃   ┃      F15      ┃      F7       ┃      F8       ┃      F9       ┃      F12      ┃
            //             ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫
            //             ┃       ;       ┃       4       ┃       5       ┃       6       ┃       =       ┃   ┃      F14      ┃      F4       ┃      F5       ┃      F6       ┃      F11      ┃
            // ┏━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━┓
            // ┃     [     ┃       `       ┃       1       ┃       2       ┃       3       ┃       \       ┃   ┃      F13      ┃      F1       ┃      F2       ┃      F3       ┃      F10      ┃           ┃
            // ┗━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┫   ┣━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━╋━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━┛
            //                                             ┃       .       ┃       0       ┃       -       ┃   ┃     ENTER     ┃     BSPC      ┃      DEL      ┃
            //                                             ┗━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┛   ┗━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┻━━━━━━━━━━━━━━━┛

            bindings = <
       &trans  &msc SCRL_LEFT  &mmv MOVE_UP    &msc SCRL_RIGHT  &td Q H                     &kp RC(LBKT)  &kp RG(RS(TAB))  &kp RG(TAB)  &kp RC(RBKT)  &trans
       &trans  &mmv MOVE_LEFT  &mmv MOVE_DOWN  &mmv MOVE_RIGHT  &kp DEL                     &kp LEFT      &kp DOWN         &kp UP       &kp RIGHT     &trans
