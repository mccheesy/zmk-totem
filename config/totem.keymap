    combos {
        compatible = "zmk,combos";

        function_layer {
            bindings = <&sl 3>;
            key-positions = <1 2 3>;
            layers = <0>;
        };

        l_alt_tab {
            bindings = <&alt_tab>;
            key-positions = <12 13>;
        };

        l_shift_alt_tab {
            bindings = <&shift_alt_tab>;
            key-positions = <11 12>;
        };

        l_launcher {
            bindings = <&kp LG(SPACE)>;
            key-positions = <11 13>;
        };

        r_alt_tab {
            bindings = <&alt_tab>;
            key-positions = <17 18>;
        };

        r_shift_alt_tab {
            bindings = <&shift_alt_tab>;
            key-positions = <16 17>;
        };

        r_launcher {
            bindings = <&kp LG(SPACE)>;
            key-positions = <16 18>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        base {
            display-name = "base";

            //         ┏━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓ ┏━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓
            //         ┃   Q   ┃   W   ┃   F   ┃   P   ┃   B   ┃ ┃   J   ┃   L   ┃   U   ┃   Y   ┃   '   ┃
            //         ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫
            //         ┃   A   ┃   R   ┃   S   ┃   T   ┃   G   ┃ ┃   M   ┃   N   ┃   E   ┃   I   ┃   O   ┃
            // ┏━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┓
            // ┃       ┃   Z   ┃   X   ┃   C   ┃   D   ┃   V   ┃ ┃   K   ┃   H   ┃   ,   ┃   .   ┃   /   ┃       ┃
            // ┗━━━━━━━┻━━━━━━━┻━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┻━━━━━━━┻━━━━━━━┛
            //                         ┃  ESC  ┃  SPC  ┃  TAB  ┃ ┃  RET  ┃ BSPC  ┃  DEL  ┃
            //                         ┃  CTL  ┃  NAV  ┃  SFT  ┃ ┃  SFT  ┃   2   ┃  CTL  ┃
            //                         ┗━━━━━━━┻━━━━━━━┻━━━━━━━┛ ┗━━━━━━━┻━━━━━━━┻━━━━━━━┛

            bindings = <
       &kp Q            &kp W       &kp F      &kp P            &kp B            &kp J          &kp L           &kp U      &kp Y       &as SQT
       &mt LGUI A       &mt LALT R  &lt 2 S    &kp T            &kp G            &kp M          &kp N           &lt 2 E    &mt LALT I  &mt RGUI O
&none  &mt LC(LSHFT) Z  &kp X       &kp C      &kp D            &kp V            &kp K          &kp H           &as_cq     &as_de      &as_ft      &none
                                    &lt 1 ESC  &mt LCTRL SPACE  &mt LSHFT TAB    &mt RSHFT RET  &mt LCTRL BSPC  &lt 1 DEL
            >;
        };

        num {
            display-name = "number";

            //         ┏━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓ ┏━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓
            //         ┃   ⌘   ┃   ⌥   ┃  ⇧-⇥  ┃   ⇥   ┃ PSCRN ┃ ┃   <   ┃   [   ┃   _   ┃   ]   ┃   >   ┃
            //         ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫
            //         ┃   ^   ┃   @   ┃   #   ┃   %   ┃   $   ┃ ┃       ┃       ┃       ┃       ┃       ┃
            //         ┃   1   ┃   2   ┃   3   ┃   4   ┃   5   ┃ ┃   {   ┃   (   ┃   :   ┃   )   ┃   }   ┃
            // ┏━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┓
            // ┃       ┃   `   ┃   &   ┃   *   ┃   |   ┃   \   ┃ ┃       ┃       ┃       ┃       ┃       ┃       ┃
            // ┃       ┃   6   ┃   7   ┃   8   ┃   9   ┃   0   ┃ ┃   +   ┃   =   ┃   ;   ┃       ┃       ┃       ┃
            // ┗━━━━━━━┻━━━━━━━┻━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫ ┣━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┻━━━━━━━┻━━━━━━━┛
            //                         ┃       ┃   +   ┃       ┃ ┃       ┃       ┃       ┃
            //                         ┃   ⌃   ┃   =   ┃   -   ┃ ┃       ┃       ┃       ┃
            //                         ┗━━━━━━━┻━━━━━━━┻━━━━━━━┛ ┗━━━━━━━┻━━━━━━━┻━━━━━━━┛

            bindings = <
       &sk LGUI  &sk LALT  &kp LS(TAB)  &kp TAB  &mt LC(PSCRN) PSCRN    &mt PSCRN RC(PSCRN)  &kp TAB    &kp RS(TAB)  &sk LALT   &sk RGUI
       &as_1c    &as N2    &as N3       &as_4p   &as_5d                 &kp LEFT             &kp DOWN   &kp UP       &kp RIGHT  &none
&none  &as_6g    &as N7    &as N8       &as_9p   &as_0b                 &kp HOME             &kp PG_DN  &kp PG_UP    &kp END    &none     &none
                           &trans       &trans   &trans                 &trans               &trans     &trans
            >;
        };

